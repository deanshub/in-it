{
  "name": "in-it-web",
  "version": "1.0.39",
  "private": true,
  "scripts": {
    "dev": "npm run prebuild && next dev -p 3001",
    "build": "npm run prebuild && next build",
    "prebuild": "cp -r ../../node_modules/webpack-bundle-analyzer/public/* public/",
    "start": "next start",
    "lint": "next lint",
    "genereateKey": "jose newkey -s 512 -t oct -a HS512"
  },
  "in-it": {
    "track": "**/*",
    "limits": [
      {
        "server/chunks/**/*": {
          "maxSize": "12mb",
          "maxDifference": "10%"
        }
      },
      {
        "static/chunks/app/layout*": {
          "maxSize": "25kb",
          "maxDifference": "10%",
          "ignorePattern": "layout(\\-.+).js$"
        }
      },
      {
        "static/chunks/app/page*": {
          "maxSize": "5kb",
          "maxDifference": "10%",
          "prohibitedModules": [
            "lodash"
          ]
        }
      },
      {
        "static/**/*": {
          "maxSize": "1mb",
          "maxDifference": "20%"
        }
      }
    ]
  },
  "dependencies": {
    "@mdx-js/loader": "^2.3.0",
    "@mdx-js/react": "^2.3.0",
    "@next/mdx": "^13.4.3",
    "@nivo/core": "^0.83.0",
    "@nivo/line": "^0.83.0",
    "@octokit/auth-app": "^4.0.13",
    "@octokit/rest": "^19.0.11",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-slot": "^1.0.1",
    "@radix-ui/react-tooltip": "^1.0.6",
    "@types/mdx": "^2.0.5",
    "@types/node": "18.16.3",
    "@types/react": "18.2.5",
    "@types/react-dom": "18.2.3",
    "@vercel/analytics": "^1.0.1",
    "@vercel/blob": "^0.8.3",
    "autoprefixer": "10.4.14",
    "class-variance-authority": "^0.6.0",
    "clsx": "^1.2.1",
    "date-fns": "^2.30.0",
    "eslint": "8.39.0",
    "eslint-config-next": "13.4.0",
    "kuuid": "^1.0.1",
    "lucide-react": "^0.220.0",
    "mongoose": "^7.2.0",
    "next": "13.4.5",
    "next-auth": "^4.22.1",
    "nissix-graph": "1.0.39",
    "octokit": "^2.0.19",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.8.0",
    "tailwind-merge": "^1.12.0",
    "tailwindcss": "3.3.2",
    "tailwindcss-animate": "^1.0.5",
    "typescript": "5.0.4",
    "webpack-bundle-analyzer": "patch:webpack-bundle-analyzer@npm%3A4.9.0#../../.yarn/patches/webpack-bundle-analyzer-npm-4.9.0-76c6aafc1f.patch"
  },
  "devDependencies": {
    "@octokit/request-error": "^4.0.2",
    "@types/webpack-bundle-analyzer": "^4.6.0",
    "in-it-shared-types": "1.0.39",
    "next-in-it-stats": "1.0.39",
    "node-jose-tools": "^2.0.2"
  }
}
