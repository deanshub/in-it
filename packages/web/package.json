{
  "name": "in-it-web",
  "version": "1.0.26",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "postbuild": "cp -r ../../node_modules/webpack-bundle-analyzer/public .next/server/app/bundle-analyzer/public",
    "start": "next start",
    "lint": "next lint",
    "genereateKey": "jose newkey -s 512 -t oct -a HS512"
  },
  "in-it": {
    "track": "**/*",
    "limits": [
      {
        "server/chunks/**/*": {
          "maxSize": "10mb",
          "maxDifference": "10%"
        }
      },
      {
        "static/chunks/app/layout*": {
          "maxSize": "2kb",
          "maxDifference": "10%",
          "ignorePattern": "layout(\\-.+).js$"
        }
      },
      {
        "static/chunks/app/page*": {
          "maxSize": "3.5kb",
          "maxDifference": "10%",
          "prohibitedModules": [
            "lodash"
          ]
        }
      },
      {
        "static/**/*": {
          "maxSize": "1mb",
          "maxDifference": "20%"
        }
      }
    ]
  },
  "dependencies": {
    "@mdx-js/loader": "^2.3.0",
    "@mdx-js/react": "^2.3.0",
    "@next/mdx": "^13.4.3",
    "@nivo/core": "^0.83.0",
    "@nivo/line": "^0.83.0",
    "@radix-ui/react-slot": "^1.0.1",
    "@types/mdx": "^2.0.5",
    "@types/node": "18.16.3",
    "@types/react": "18.2.5",
    "@types/react-dom": "18.2.3",
    "@vercel/blob": "^0.8.3",
    "autoprefixer": "10.4.14",
    "class-variance-authority": "^0.6.0",
    "clsx": "^1.2.1",
    "eslint": "8.39.0",
    "eslint-config-next": "13.4.0",
    "kuuid": "^1.0.1",
    "lucide-react": "^0.220.0",
    "mongoose": "^7.2.0",
    "next": "13.4.0",
    "next-auth": "^4.22.1",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.8.0",
    "tailwind-merge": "^1.12.0",
    "tailwindcss": "3.3.2",
    "tailwindcss-animate": "^1.0.5",
    "typescript": "5.0.4",
    "webpack-bundle-analyzer": "^4.8.0"
  },
  "devDependencies": {
    "@types/webpack-bundle-analyzer": "^4.6.0",
    "in-it-shared-types": "1.0.26",
    "next-in-it-stats": "1.0.26",
    "node-jose-tools": "^2.0.2"
  }
}
